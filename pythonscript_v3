from easysnmp import snmp_get

result = snmp_get('.1.3.6.1.2.1.1.1.0', hostname='192.168.0.3', community='public', version=1)  #Diverses Pi3
result1 = snmp_get('.1.3.6.1.2.1.1.5.0', hostname='192.168.0.3', community='public', version=1) #Hostname Pi3
result2 = snmp_get('.1.3.6.1.2.1.1.6.0', hostname='192.168.0.3', community='public', version=1) #System-Aufenthalt Pi3
result3 = snmp_get('.1.3.6.1.2.1.1.3.0', hostname='192.168.0.3', community='public', version=1) #Uptime Pi3
result4 = snmp_get('.1.3.6.1.2.1.1.1.0', hostname='192.168.0.2', community='public', version=1) #Diverses Switch
result5 = snmp_get('.1.3.6.1.2.1.1.5.0', hostname='192.168.0.2', community='public', version=1) #Hostname Switch
result6 = snmp_get('.1.3.6.1.2.1.1.6.0', hostname='192.168.0.2', community='public', version=1) #System-Aufenthalt Switch
result7 = snmp_get('.1.3.6.1.2.1.1.3.0', hostname='192.168.0.2', community='public', version=1) #Uptime Switch
result8 = snmp_get('.1.3.6.1.2.1.1.1.0', hostname='192.168.0.1', community='public', version=1) #Diverses Router
result9 = snmp_get('.1.3.6.1.2.1.1.5.0', hostname='192.168.0.1', community='public', version=1) #Hostname Router
result10 = snmp_get('.1.3.6.1.2.1.1.6.0', hostname='192.168.0.1', community='public', version=1) #System-Aufenthalt Router
result11 = snmp_get('.1.3.6.1.2.1.1.3.0', hostname='192.168.0.1', community='public', version=1) #Uptime Router

print (result.value)
print (result1.value)
print (result2.value)
print (result3.value)
print (result4.value)
print (result5.value)
print (result6.value)
print (result7.value)
print (result8.value)
print (result9.value)
print (result10.value)
print (result11.value)
